    "build:prod": "EAS_BUILD_PROFILE=\"production\" eas build --profile production --platform",
    "update:prod": "EAS_BUILD_PROFILE=\"production\" eas update --branch production --platform",
    "doc": "npx expo install --fix",
    "lint": "npx eslint --fix",
    "pretty": "prettier . --fix",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "check-updates": "npm-check-updates -u",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "npx expo run:ios"
